        "bcrypt": "^4.0.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.4.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.20.10",
        "module-alias": "^2.2.2",
        "pino": "^6.3.2",
        "pino-pretty": "^4.0.0",
        "read-file": "^0.2.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "sqlite3": "^4.1.1",
        "validator": "^13.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^3.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.4",
        "@types/fs-extra": "^8.1.0",
        "@types/jsonwebtoken": "^8.3.9",
        "@types/knex": "^0.16.1",
        "@types/node": "^13.11.0",
        "@types/pino": "^6.0.1",
        "@types/shelljs": "^0.8.7",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-client": "^1.4.32",
        "fs-extra": "^9.0.0",
        "shelljs": "^0.8.3",
        "ts-node": "^8.8.2",
        "ts-node-dev": "^1.0.0-pre.44",
        "tslint": "^6.1.1",
        "typescript": "^3.8.3"
    },
    "scripts": {
        "serve": "nodemon ./dist/index.js",
        "dev": "ts-node src/index.ts",
        "dev-watch": "ts-node-dev --respawn --transpileOnly src/index.ts",
        "build": "tsc",
        "migration-up": "knex --knexfile ./knexfile.ts --cwd src/db migrate:latest",
        "migration-down": "knex --knexfile ./knexfile.ts --cwd src/db migrate:rollback --all",
        "migration-make": "knex --knexfile ./knexfile.ts --cwd src/db migrate:make",
        "seed-run": "knex --knexfile ./knexfile.ts --cwd src/db seed:run",
        "seed-make": "knex --knexfile ./knexfile.ts --cwd src/db seed:make"
    },
    "_moduleAliases": {
        "@app": "src/app",
        "@controllers": "src/app/controllers",
        "@services": "src/app/services"
    }
}
